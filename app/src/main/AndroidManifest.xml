<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.trabajos.labcorte01">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="App"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LABCorte01"
        tools:targetApi="31">
        <activity
            android:name=".formulario_CRUD"
            android:exported="false">
            <meta-data
                android:name="android.app.lib_name"
                android:value="" />
        </activity>
        <activity
            android:name=".MenuLogs"
            android:exported="false"
            android:label="Menu Logs"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MenuSeguridad"
            android:exported="false"
            android:label="Menu de Seguridad"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MenuEventos"
            android:exported="false"
            android:label="Menu de Eventos"
            android:parentActivityName=".MainActivity" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>